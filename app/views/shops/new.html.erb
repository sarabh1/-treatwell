 <%= simple_form_for @shop do |f| %>
          <%= f.input :name %>
          <%= f.input :address %>
          <%= f.input :email %>
          <%= f.input :description %>
          <%= f.input :telephone %>
          <%= f.input :photos, as: :file, input_html: { multiple: true } %>
          <br>
          <%= f.button :submit %>
        <% end %>
